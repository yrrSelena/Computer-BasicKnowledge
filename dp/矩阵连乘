#include <iostream>#include <stdio.h>using namespace std;#define MAX 100int a[105];int m[105][105];int s[105][105];int main(){    int n,t;    while(scanf("%d",&n)!=EOF)    {        for(int i=0;i<n;i++)        {            scanf("%d",&a[i]);            m[i][i]=0;        }        int j;        for(int r=2;r<n;r++)        {            for(int i=1;i<n;i++)            {                j=i+r-1;                m[i][j]=m[i][i]+m[i+1][j]+a[i-1]*a[i]*a[j];                for(int k=i+1;k<j;k++)                {                    t=m[i][k]+m[k+1][j]+a[i-1]*a[k]*a[j];                    if(t<m[i][j])                    {                        m[i][j]=t;                        s[i][j]=k;                    }                }            }        }        cout<<m[1][n-1]<<endl;    }    return 0;}
